<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- theme meta -->
    <meta name="theme-name" content="Gimnasio SVAM" />
    <title>Cafetería </title>
    
    <link href="~/WebStyles/css/lib/calendar2/pignose.calendar.min.css" rel="stylesheet">
    <link href="~/WebStyles/css/lib/chartist/chartist.min.css" rel="stylesheet">
    <link href="~/WebStyles/css/lib/font-awesome.min.css" rel="stylesheet">
    <link href="~/WebStyles/css/lib/themify-icons.css" rel="stylesheet">
    <link href="~/WebStyles/css/lib/owl.carousel.min.css" rel="stylesheet" />
    <link href="~/WebStyles/css/lib/owl.theme.default.min.css" rel="stylesheet" />
    <link href="~/WebStyles/css/lib/weather-icons.css" rel="stylesheet" />
    <link href="~/WebStyles/css/lib/menubar/sidebar.css" rel="stylesheet">
    <link href="~/WebStyles/css/lib/bootstrap.min.css" rel="stylesheet">
    <link href="~/WebStyles/css/lib/helper.css" rel="stylesheet">
    <link href="~/WebStyles/css/style.css" rel="stylesheet">
</head>

<body>

    <div class="sidebar sidebar-hide-to-small sidebar-shrink sidebar-gestures">
        <div class="nano">
            <div class="nano-content">
                <ul>
                    <div class="logo">
                        <a href="index.html">
                            <img src="~/WebStyles/images/logo.png" alt="" /> <span style="margin-left: 20px">Focus</span>
                        </a>
                    </div>


                    <li class="label">Apps</li>

                    <li><a href="app-event-calender.html"><i class="ti-calendar"></i> Calender </a></li>
                    <li><a href="app-email.html"><i class="ti-email"></i> Email</a></li>
                    <li><a href="app-profile.html"><i class="ti-user"></i> Profile</a></li>

            </div>
        </div>
    </div>
    <!-- /# sidebar -->
    <div class="header">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <div class="float-left">
                        <div class="hamburger sidebar-toggle">
                            <span class="line"></span>
                            <span class="line"></span>
                            <span class="line"></span>
                        </div>
                    </div>
                    <div class="float-right">
                        <div class="dropdown dib">
                            <div class="header-icon" data-toggle="dropdown">
                                <i class="ti-bell"></i>
                                <div class="drop-down dropdown-menu dropdown-menu-right">
                                    <div class="dropdown-content-heading">
                                        <span class="text-left">Recent Notifications</span>
                                    </div>
                                    <div class="dropdown-content-body">
                                        <ul>



                                            <li>
                                                <a href="#">
                                                    <img class="pull-left m-r-10 avatar-img"
                                                         src="~/WebStyles/images/avatar/3.jpg" alt="" />
                                                    <div class="notification-content">
                                                        <small class="notification-timestamp pull-right">
                                                            02:34
                                                            PM
                                                        </small>
                                                        <div class="notification-heading">Mr. John</div>
                                                        <div class="notification-text">
                                                            Hi Teddy, Just wanted to let you
                                                            ...
                                                        </div>
                                                    </div>
                                                </a>
                                            </li>
                                            <li class="text-center">
                                                <a href="#" class="more-link">See All</a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="dropdown dib">
                            <div class="header-icon" data-toggle="dropdown">
                                <i class="ti-email"></i>
                                <div class="drop-down dropdown-menu dropdown-menu-right">
                                    <div class="dropdown-content-heading">
                                        <span class="text-left">2 New Messages</span>
                                        <a href="email.html">
                                            <i class="ti-pencil-alt pull-right"></i>
                                        </a>
                                    </div>
                                    <div class="dropdown-content-body">
                                        <ul>
                                            <li class="notification-unread">
                                                <a href="#">
                                                    <img class="pull-left m-r-10 avatar-img"
                                                         src="~/WebStyles/images/avatar/1.jpg" alt="" />
                                                    <div class="notification-content">
                                                        <small class="notification-timestamp pull-right">
                                                            02:34
                                                            PM
                                                        </small>
                                                        <div class="notification-heading">Michael Qin</div>
                                                        <div class="notification-text">
                                                            Hi Teddy, Just wanted to let you
                                                            ...
                                                        </div>
                                                    </div>
                                                </a>
                                            </li>



                                            <li class="text-center">
                                                <a href="#" class="more-link">See All</a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="dropdown dib">
                            <div class="header-icon" data-toggle="dropdown">
                                <span class="user-avatar">
                                    John
                                    <i class="ti-angle-down f-s-10"></i>
                                </span>
                                <div class="drop-down dropdown-profile dropdown-menu dropdown-menu-right">
                                    <div class="dropdown-content-heading">
                                        <span class="text-left">John Goldberg</span>
                                        <p class="trial-day">32 años</p>
                                    </div>
                                    <div class="dropdown-content-body">
                                        <ul>
                                            <li>
                                                <a href="#">
                                                    <i class="ti-user"></i>
                                                    <span>Profile</span>
                                                </a>
                                            </li>


                                            <li>
                                                <a href="#">
                                                    <i class="ti-settings"></i>
                                                    <span>Setting</span>
                                                </a>
                                            </li>


                                            <li>
                                                <a href="#">
                                                    <i class="ti-power-off"></i>
                                                    <span>Logout</span>
                                                </a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="content-wrap">
        <div style="margin:20px" class="main">
            @RenderBody()

        </div>
    </div>

    

    <!-- jquery vendor -->
    <script src="~/WebStyles/js/lib/jquery.min.js"></script>
    <script src="~/WebStyles/js/lib/jquery.nanoscroller.min.js"></script>
    <!-- nano scroller -->
    <script src="~/WebStyles/js/lib/menubar/sidebar.js"></script>
    <script src="~/WebStyles/js/lib/preloader/pace.min.js"></script>
    <!-- sidebar -->

    <script src="~/WebStyles/js/lib/bootstrap.min.js"></script>
    <script src="~/WebStyles/js/scripts.js"></script>
    <!-- bootstrap -->

    <script src="~/WebStyles/js/lib/calendar-2/moment.latest.min.js"></script>
    <script src="~/WebStyles/js/lib/calendar-2/pignose.calendar.min.js"></script>
    <script src="~/WebStyles/js/lib/calendar-2/pignose.init.js"></script>


    <script src="~/WebStyles/js/lib/weather/jquery.simpleWeather.min.js"></script>
    <script src="~/WebStyles/js/lib/weather/weather-init.js"></script>
    <script src="~/WebStyles/js/lib/circle-progress/circle-progress.min.js"></script>
    <script src="~/WebStyles/js/lib/circle-progress/circle-progress-init.js"></script>
    <script src="~/WebStyles/js/lib/sparklinechart/jquery.sparkline.min.js"></script>
    <script src="~/WebStyles/js/lib/sparklinechart/sparkline.init.js"></script>
    <script src="~/WebStyles/js/lib/owl-carousel/owl.carousel.min.js"></script>
    <script src="~/WebStyles/js/lib/owl-carousel/owl.carousel-init.js"></script>
    <!-- scripit init-->
</body>

</html>